<Window x:Class="Gyumin.Graphics.RayTracer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:Gyumin.Graphics.RayTracer"
        Title="MainWindow" SizeToContent="WidthAndHeight" ResizeMode="CanMinimize">
    <Grid>
        <Image x:Name="xImage"
               Width="{x:Static local:Config.ImageWidthD}"
               Height="{x:Static local:Config.ImageHeightD}" />
    </Grid>
</Window>
